<% layout("layout/boilerplate.ejs") %>

    <div class="col-md-8 m-auto mb-5">
        <!-- to send file to the server we set encoding type to multipart/form-data -->
        <h1 class="mb-5">Select The Fields</h1>
        <form action="/jd/<%= jd.id %>/downloadAppliedStudents" method="POST">
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="registration_no" value="username"
                        id="registration_no">
                    <label class="form-check-label" for="registration_no">
                        Registration No.
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="name" value="name" id="name">
                    <label class="form-check-label" for="name">
                        Name
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="phone_no" value="phone_no" id="phone_no">
                    <label class="form-check-label" for="phone_no">
                        Phone No
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="email" value="email" id="email">
                    <label class="form-check-label" for="email">
                        Email
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="gender" value="gender" id="gender">
                    <label class="form-check-label" for="gender">
                        Gender
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="course" value="course" id="course">
                    <label class="form-check-label" for="course">
                        Course
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="tenth" value="tenth" id="tenth">
                    <label class="form-check-label" for="tenth">
                        Tenth
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="twelve" value="twelve" id="twelve">
                    <label class="form-check-label" for="twelve">
                        Twelve
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="graduation" value="graduation"
                        id="graduation">
                    <label class="form-check-label" for="graduation">
                        Graduation
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="masters" value="masters" id="masters">
                    <label class="form-check-label" for="masters">
                        Masters
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="resume" value="resume" id="resume">
                    <label class="form-check-label" for="resume">
                        Resume
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="year_of_passing" value="year_of_passing"
                        id="year_of_passing">
                    <label class="form-check-label" for="year_of_passing">
                        Year of Passing
                    </label>
                </div>
            </div>
            <div class="mb-3">
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" name="skills" value="skills" id="skills">
                    <label class="form-check-label" for="skills">
                        Skills
                    </label>
                </div>
            </div>

            <button class="btn btn-outline-success">Download</button>
        </form>
    </div>